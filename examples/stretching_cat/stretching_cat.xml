<camotics>
  <!-- Note, all values are in mm regardless of 'units' option. -->
  <!-- NC Files -->
  <nc-files>
    stretching_cat.tpl
  </nc-files>

  <!-- Renderer -->
  <resolution v='0.5'/>
  <resolution-mode v='MANUAL'/>

  <!-- Workpiece -->
  <automatic-workpiece v='false'/>
  <workpiece-max v='(100,200,0)'/>
  <workpiece-min v='(0,0,-10)'/>

  <tool_table>
    <tool length='10' number='1' radius='0.79375' shape='CYLINDRICAL' units='MM'/>
  </tool_table>
</camotics>