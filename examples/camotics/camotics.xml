<camotics>
  <!-- Note, all values are in mm regardless of 'units' option. -->
  <!-- NC Files -->
  <nc-files>
    camotics.ngc
  </nc-files>

  <!-- Renderer -->
  <resolution v='0.125723'/>
  <resolution-mode v='HIGH'/>

  <!-- Workpiece -->
  <automatic-workpiece v='false'/>
  <workpiece-max v='(128,20,-3)'/>
  <workpiece-min v='(-10,-4,-6)'/>

  <tool_table>
    <tool length='10' number='1' radius='5' shape='CONICAL' units='MM'/>
  </tool_table>
</camotics>