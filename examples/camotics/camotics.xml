<camotics>
  <!-- Note, all values are in mm regardless of 'units' option. -->
  <!-- NC Files -->
  <nc-files>
    camotics.tpl
  </nc-files>

  <!-- Renderer -->
  <resolution v='0.175'/>
  <resolution-mode v='MANUAL'/>

  <!-- Workpiece -->
  <automatic-workpiece v='false'/>
  <workpiece-max v='(122,18,0)'/>
  <workpiece-min v='(-3,-3,-3)'/>

  <tool_table>
    <tool length='10' number='1' radius='1.25' shape='BALLNOSE' units='MM'/>
    <tool length='10' number='2' radius='1' shape='CYLINDRICAL' units='MM'/>
    <tool length='10' number='3' radius='1' shape='CYLINDRICAL' units='MM'/>
    <tool length='1' number='4' radius='3.5' shape='CYLINDRICAL' units='MM'/>
    <tool length='10' number='5' radius='1' shape='SNUBNOSE' snub_diameter='9' units='MM'/>
    <tool length='10' number='6' radius='1' shape='CYLINDRICAL' units='MM'/>
    <tool length='10' number='7' radius='2.5' shape='SNUBNOSE' snub_diameter='2' units='MM'/>
    <tool length='10' number='8' radius='1' shape='CYLINDRICAL' units='MM'/>
    <tool length='10' number='9' radius='1' shape='CYLINDRICAL' units='MM'/>
  </tool_table>
</camotics>